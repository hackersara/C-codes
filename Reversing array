
#include <iostream>
using namespace std;
int main() {
    
     // size of array 
    int n;
    cin>>n;
    int arr[n];
    
    
    // taking input in an array
    for(int i=0; i<=n-1; i++)
    {
    cin>>arr[i];
    }
    
    //reversing the array
     for(int i=n-1; i>=0; i--)
     {
        cout<<arr[i]<<" ";
     }
   
    
    return 0;
}
