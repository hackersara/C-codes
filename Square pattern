
#include <iostream>
using namespace std;
int main() 
{
   
   int tr;
   cin>>tr;
   int row;
   row=1;
   
   while(row<=tr)
   {
    // SIMILAR NUMBERS 
    int i=1;
   while(i<=row)
   {
    cout<<row<<" ";
    i=i+1;
   }
   
   //  normal numbers
   int j=1;
   int num=row+1; // num =row+1 because in every row number is starting from row1=2 , row2=3 
   
   while(j<=tr-row)
   {
      cout<<num<<" ";
      num++;
      j++;
   }
   cout<<endl;
   row=row+1;
   }
 
 return 0;
}
